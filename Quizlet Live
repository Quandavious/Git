javascript:(async function(){const pin=prompt('Enter PIN, like: 123-456','').match(/[0-9]/g).join('');if(pin.length!==6){alert('Invalid Pin, try again');return}let gameId=await fetch(`https://quizlet.com/webapi/3.2/game-instances?filters={%22gameCode%22:${%20pin%20},%22isInProgress%22:true,%22isDeleted%22:false}&perPage=500`);gameId=await%20gameId.json();const%20set=gameId.responses[0].models.gameInstance[0]?.itemId;if(!set){alert(%27Set%20not%20found,%20make%20sure%20the%20pin%20is%20correct%20and%20try%20again%27);return}let%20setPage=await%20fetch(`https://quizlet.com/${%20set%20}`);setPage=await%20setPage.text();try{eval(setPage.slice(setPage.indexOf(%27(function(){window.Quizlet[%22setPageData%22]%27),setPage.indexOf(%27setPageData%27)+setPage.slice(setPage.indexOf(%27setPageData%27)).indexOf(%27%3C/script%3E%27)))}catch{};const%20mapping=Quizlet.setPageData?.termIdToTermsMap;if(!mapping){alert(%22Set%20data%20didn%27t%20load%20properly,%20try%20again%22);return}let%20terms={};let%20definitions={};for(const%20item%20of%20Object.values(mapping)){terms[item.word]=item.definition;definitions[item.definition]=item.word}setInterval(function(){try{const%20question=document.getElementsByClassName(%27FormattedText%20notranslate%20StudentPrompt-text%20lang-en%27)[0].textContent;const%20answers=Array.from(document.getElementsByClassName(%27FormattedText%20notranslate%20StudentAnswerOption-text%20lang-en%27));if(question%20in%20terms){answers.forEach((box)=%3E{if(box.textContent===terms[question]){box.style.fontWeight=%27bolder%27}})}else%20if(question%20in%20definitions){answers.forEach((box)=%3E{if(box.textContent===definitions[question]){box.style.fontWeight=%27bolder%27}})}}catch{}},100)})();void%200
